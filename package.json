{
  "name": "styled-breakpoints",
  "version": "8.1.0",
  "description": "Simple and powerfull css breakpoints for styled-components",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "rebuild": "npm run clean && npm run build",
    "build": "cross-env tsc && npm run size",
    "clean": "cross-env rimraf dist",
    "size": "cross-env size-limit",
    "ts-check": "cross-env tsc --noEmit",
    "ts-check:watch": "npm run ts-check -- --watch",
    "test": "npm run lint:all && npm run ts-check && npm run test:code",
    "test:watch": "cross-env jest --coverage --watchAll",
    "test:code": "cross-env jest --coverage",
    "lint:file": "cross-env eslint --ext .ts --quiet --fix",
    "lint:all": "cross-env fixme=fixme eslint --ext .ts src/ --quiet --fix.",
    "commit": "cross-env git-cz",
    "coverage": "cross-env fixme=fixme cat ./coverage/lcov.info | coveralls",
    "pretty": "cross-env fixme=fixme prettier './**/**/**.{json,js,ts}' --write",
    "semantic-release": "cross-env semantic-release",
    "contributors:add": "cross-env all-contributors add",
    "contributors:generate": "cross-env all-contributors generate"
  },
  "license": "MIT",
  "homepage": "https://github.com/mg901/styled-breakpoints#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mg901/styled-breakpoints.git"
  },
  "bugs": {
    "url": "https://github.com/mg901/styled-breakpoints/issues"
  },
  "author": "Maxim Alyoshin <mgreen097@gmail.com> (https://github.com/mg901)",
  "keywords": [
    "media",
    "styled",
    "react",
    "preact",
    "media",
    "emotion",
    "javascript",
    "css-in-js",
    "breakpoint",
    "media-query",
    "breakpoints",
    "css-in-react",
    "media-queries",
    "typescript",
    "styled-components",
    "styled-breakpoints"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.0",
    "@semantic-release/npm": "^7.0.0",
    "@semantic-release/release-notes-generator": "^9.0.0",
    "@size-limit/preset-small-lib": "^4.0.1",
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "all-contributors-cli": "^6.14.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.7",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.4",
    "onchange": "^6.1.0",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.1",
    "size-limit": "^4.0.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5"
  }
}
